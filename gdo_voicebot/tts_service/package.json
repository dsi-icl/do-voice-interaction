    "lint:fix": "eslint --fix ./src/**/*.js ./src/*.js"
  },
  "dependencies": {
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "esm": "3.2.25",
    "express": "4.16.1",
    "gtts": "^0.2.1",
    "http-errors": "1.6.3",
    "lodash": "4.17.19",
    "morgan": "1.9.1",
    "uuid": "8.1.0",
    "yargs-parser": "^18.1.3"
  },
  "devDependencies": {
    "@zeit/ncc": "0.22.1",
    "babel-eslint": "10.0.3",
    "eslint": "7.0.0",
    "nodemon": "2.0.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__utils"
